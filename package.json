{
  "name": "playground",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon --watch src src/index.ts --exec babel-node --extensions=\".ts,.js\" -e js,ts,gql",
    "build": "babel src/schema -d dist/schema --copy-files && babel src -d dist --extensions=\".ts,.js\"",
    "start": "node dist/index.js",
    "table": "ts-node framework/generate_table_cli.ts",
    "graph": "ts-node framework/generate_graph_type.ts",
    "migrate": "knex migrate:latest",
    "lint": "eslint src/ --ext .ts --ext .tsx --ignore-pattern \"*.d.ts\" --max-warnings=0"
  },
  "dependencies": {
    "@babel/node": "^7.12.6",
    "@babel/plugin-proposal-async-generator-functions": "^7.12.1",
    "@babel/plugin-proposal-optional-chaining": "^7.10.3",
    "@babel/plugin-transform-typescript": "^7.14.3",
    "@babel/polyfill": "^7.12.1",
    "@types/request-promise": "^4.1.47",
    "apollo-server": "^2.19.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "bcryptjs": "^2.4.3",
    "dataloader": "^1.4.0",
    "dotenv": "^8.2.0",
    "fs": "^0.0.1-security",
    "graphql": "^15.4.0",
    "graphql-request": "^3.6.1",
    "knex": "^0.21.15",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.33",
    "mysql2": "^1.6.5",
    "nodemon": "^2.0.6",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "ts-node": "^9.0.0",
    "typescript": "^4.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@typescript-eslint/eslint-plugin": "^2.17.0",
    "@typescript-eslint/parser": "^2.17.0",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "prettier": "^1.19.1",
    "@babel/preset-env": "^7.4.1",
    "@babel/preset-typescript": "^7.3.3",
    "@types/bcryptjs": "^2.4.2",
    "crypto": "^1.0.1",
    "momentjs": "^2.0.0"
  }
}
